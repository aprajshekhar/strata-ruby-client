                    <xs:sequence>
                        <xs:element name="caseId" type="xs:string" />
                        <xs:element name="uuid" type="xs:string" />
                        <xs:element name="etag" type="xs:string" />
                        <xs:element name="uri" type="xs:anyURI" />
                        <xs:element name="fileDate" type="xs:dateTime" />
                        <xs:element name="fileName" type="xs:string" />
                        <xs:element name="description" type="xs:string" />
                        <xs:element name="mimeType" type="xs:string" />
                        <xs:element name="length" type="xs:long" />
                        <xs:element name="active" type="xs:boolean" />
                        <xs:element name="deprecated" type="xs:boolean" />
                        <xs:element name="private" type="xs:boolean" minOccurs="0"  maxOccurs="1" default="false"/>
                    </xs:sequence>
                    <xs:attribute name="caseNumber" type="xs:string" use="required" />
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
        
    <xs:element name="suggestions">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of suggestions.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence>
                        <xs:element ref="tns:suggestion" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="suggestion">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A suggestion referring to a solution and related meta data.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:trackedEntity">
                    <xs:sequence>
                        <xs:element name="algorithmScore" type="xs:int" />
                        <xs:element name="analysisAlgorithm" type="xs:string" />
                        <xs:element name="analysisAlgorithmVersion" type="xs:string" />
                        <xs:element name="analysisCategory" type="xs:string" />
                        <xs:element name="analysisModule" type="xs:string" />
                        <xs:element name="analysisService" type="xs:string" />
                        <xs:element name="analysisServiceVersion" type="xs:string" />
                        <xs:element name="bucket" type="xs:string" />
                        <xs:element name="caseNumber" type="xs:string" />
                        <xs:element name="client" type="xs:string" />
                        <xs:element name="clientVersion" type="xs:string" />
                        <xs:element name="display" type="xs:boolean" />
                        <xs:element name="keywords" type="xs:string" />
                        <xs:element name="location" type="xs:string" />
                        <xs:element name="luceneScore" type="xs:int" />
                        <xs:element name="note" type="xs:string"/>
                        <xs:element name="relevanceScore" type="xs:int" />
                        <xs:element name="resource" type="xs:string" />
                        <xs:element name="rule" type="xs:string" />
                        <xs:element name="ruleVersion" type="xs:string" />
                        <xs:element name="scoringAlgorithmVersion" type="xs:int"/>
                        <xs:element name="solutionId" type="xs:string" />
                        <xs:element name="solutionKcsState" type="xs:string" />
                        <xs:element name="solutionOwnerSSOName" type="xs:string" />
                        <xs:element name="solutionTitle" type="xs:string" />
                        <xs:element name="solutionUrl" type="xs:string" />
                        <xs:element name="tracebackUrl" type="xs:string" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
        
    <xs:element name="groups">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of groups.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence>
                        <xs:element ref="tns:group" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="group">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A group that may contain zero or more cases.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence><!--
						<xs:element name="id" type="xs:string" />
						-->
                        <xs:element name="number" type="xs:string" />
                        <xs:element name="name" type="xs:string" />
                        <xs:element name="uri" type="xs:anyURI" />
                        <xs:element name="isPrivate" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    </xs:sequence>
					<!--<xs:attribute name="number" type="xs:string" use="optional" />
				-->
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
	
    <xs:element name="account">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>An account that may organizes users. It is represented in a simplified manner on the Strata API.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence>
                        <xs:element name="hasGroupACLs" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    </xs:sequence>
                    <xs:attribute name="number" type="xs:string" />
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
	
    <xs:element name="users">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of users.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence>
                        <xs:element ref="tns:user" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
	
    <xs:element name="user">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>The user entity reflects a Red Hat Login user.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence>
						<!-- TODO Where does ID come from? -->
						<!-- TODO Perhaps internal only? -->
                        <xs:element name="id" type="xs:string" />
                        <xs:element name="firstName" type="xs:string" />
                        <xs:element name="lastName" type="xs:string" />
                        <xs:element name="email" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="accountId" type="xs:anyURI" minOccurs="0" maxOccurs="1" />
                        <!-- TODO Consider as enumerated type -->
                        <xs:element name="preferredLanguage" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <!-- TODO Consider as enumerated type -->
                        <xs:element name="timezone" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="orgAdmin" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                        <xs:element name="tamContact" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                        <xs:element name="access" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                        <xs:element name="hasChat" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                        <xs:element name="sessionId" type="xs:string" minOccurs="0" maxOccurs="1" />
                         
                        <!-- TODO Add roles element -->
                        <!-- TODO Consider adding link/relation element to use for account -->
                    </xs:sequence>
                    <xs:attribute name="ssoUsername" type="xs:string" use="optional" />
                    <xs:attribute name="uri" type="xs:anyURI" />
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
	
    <xs:element name="cases">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of support cases. This type serves as both a
					top-level construct in the case that a query returns a list of
					support cases as well as an embedded construct as part of a larger
					item.  This element is not required to contain any child 
					case elements.
                </xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence>
                        <xs:element ref="tns:case" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="case">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A support case.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:trackedEntity">
                    <xs:sequence>
                        <xs:element name="id" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Case identifier assigned by backing system.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>RESTful URI for this case resource.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="summary" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Title or short summary.  This field is required by the model
								but the schema allows it to be omitted so that field-level updates are possible.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Description of the problem or question.  This field is required by the model
								but the schema allows it to be omitted so that field-level updates are possible.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="status" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Case status.  Valid values are available from 
								the values service.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="product" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Product this case relates to.  Valid products 
								are available from the products resource.  This field is required by the model
								but the schema allows it to be omitted so that field-level updates are possible.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="component" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Component this case relates to (optional). Valid
								components for a given product are available from the values 
								service.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="version" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Version of the product this case relates to.
								Valid versions for a given product are available from the products
								resource.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="type" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Type of case, i.e. question, problem, etc.
								Valid types are available from the values service.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="accountNumber" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Account number this case belongs to.  This field is required 
								by the model but the schema allows it to be omitted so that field-level updates 
								are possible.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="reference" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="notes" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="escalated" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                        <xs:element name="contactName" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="contactSsoUsername" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="origin" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="owner" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="internalPriority" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="internalStatus" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="suppliedName" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="suppliedPhone" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="suppliedEmail" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="severity" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="folderNumber" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element ref="tns:comments" minOccurs="0" />
                        <xs:element ref="tns:notified_users" minOccurs="0" />
                        <xs:element ref="tns:entitlement" minOccurs="1" maxOccurs="1" />
                        <xs:element ref="tns:solutions" minOccurs="1" maxOccurs="1" />
                    </xs:sequence>
                    <xs:attribute name="caseNumber" type="xs:string" use="optional" />
                    <xs:attribute name="closed" type="xs:boolean" use="optional" />
                    <xs:attribute name="alternateId" type="xs:string" use="optional" />
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="comments">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of comments</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence>
                        <xs:element ref="tns:comment" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="comment">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A support case comment and associated metadata.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:trackedEntity">
                    <xs:sequence>
                        <xs:element name="text" type="xs:string"  minOccurs="0" maxOccurs="1"/>
                        <xs:element name="uri" type="xs:anyURI" minOccurs="0"  maxOccurs="1"/>
                        <xs:element name="public" type="xs:boolean" minOccurs="0"  maxOccurs="1" default="true"/>
                        <xs:element name="draft" type="xs:boolean" minOccurs="0"  maxOccurs="1" default="false"/>
                        <xs:element name="publishedDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
                    </xs:sequence>
                    <xs:attribute name="id" type="xs:string" />
                    <xs:attribute name="caseNumber" type="xs:string" use="required" />
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="notified_users">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of users notified by email when the parent case is modified</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence>
                        <xs:element ref="tns:link" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="products">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of products.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:element ref="tns:product" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="product">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A product.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:element name="code" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>A short, unique, human-readable code identifying this product.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>The formal product brand name.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="versions">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of product versions.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:element name="version" minOccurs="0" maxOccurs="unbounded" type="xs:string" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="values">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of name/value pairs.  Both names and values are String typed.  List can 
				contain zero or more elements.  Values are nullable.
                </xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:element name="value" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="value" type="xs:string" minOccurs="0" maxOccurs="1" />
                        </xs:sequence>
                        <xs:attribute name="type" type="xs:string" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
   
<!--    <xs:element name="NewDataSet">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="tnd:und" />
                <xs:element ref="tns:current" />
                <xs:element ref="tns:result" />
            </xs:choice>
        </xs:complexType>
    </xs:element>-->
<xs:element name="entitlements">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of Entitlements.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence>
                        <xs:element ref="tns:entitlement" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="entitlement">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>entitlement and related meta data.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:trackedEntity">
                    <xs:sequence>
                        <xs:element name="id" type="xs:string" minOccurs="0" maxOccurs="1" />
                        <xs:element name="uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                                <xs:documentation>RESTful URI for SLA.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="name" type="xs:string" />
                        <xs:element name="sla" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="supportLevel" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="serviceLevel" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="startDate" type="xs:date" />
                        <xs:element name="endDate" type="xs:date" />
                       </xs:sequence>
                    <!-- <xs:attribute name="account" type="xs:string" use="required" /> -->
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
   <xs:element name="error">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Provides error code and description for Starta API</xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:element name="code" type="xs:string" />
                <xs:element name="message" type="xs:string" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
   
    <xs:element name="systemProfiles">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of systemProfiles.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:strataEntity">
                    <xs:sequence>
                        <xs:element ref="tns:systemProfile" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
      
    <xs:element name="systemProfile">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>systemProfile and related meta data.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:trackedEntity">
                    <xs:sequence>
                        <xs:element name="accountNumber" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="caseNumber" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="attachmentHash" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="deprecated" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="hash" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        <xs:element ref="tns:SystemProfileCategory" minOccurs="0" maxOccurs="unbounded"/>
                       </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
   
    
    <xs:element name="SystemProfileCategory">
        <xs:complexType>
        <xs:sequence>
        <xs:element name="SystemProfileCategoryName" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="tns:SystemProfileCategoryDetails" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
            </xs:complexType>
    </xs:element>
    
    <xs:element name="SystemProfileCategoryDetails">
        <xs:complexType>
        <xs:sequence>
                 <xs:element name="key" type="xs:string" minOccurs="0" maxOccurs="1"/>
                 <xs:element name="value" type="xs:string" minOccurs="0" maxOccurs="1"/> 
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
</xs:schema>


