    <!-- Lightweight emulation of ATOM link -->
    <xs:element name="link">
	    <xs:complexType>
	        <xs:annotation>
	            <xs:documentation>Simple ATOM link type.</xs:documentation>
	        </xs:annotation>
	        <xs:complexContent>
	            <xs:extension base="tns:base_link">
	                <!-- The following are non-standard fields and attributes not dictated by ATOM -->
