<xs:schema targetNamespace="http://www.redhat.com/gss/strata" elementFormDefault="qualified" version="1.1" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://www.redhat.com/gss/strata"
	>

    <xs:annotation>
        <xs:documentation>Red Hat Support Services API</xs:documentation>
    
    </xs:annotation>

    <xs:complexType name="strataEntity" abstract="true">
        <xs:annotation>
            <xs:documentation>The base type for all Strata domain entities.</xs:documentation>
        </xs:annotation>
    </xs:complexType>
	
    <xs:complexType name="trackedEntity" abstract="true">
        <xs:annotation>
            <xs:documentation>A common base type for any type that needs the common created/updated
			information.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tns:strataEntity">
                <xs:sequence>
                    <xs:element name="createdBy" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="createdDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
                    <xs:element name="lastModifiedBy" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="lastModifiedDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="base_link">
	    <xs:annotation>
	        <xs:documentation>ATOM link type.</xs:documentation>
	    </xs:annotation>
	    <xs:complexContent>
	        <xs:extension base="tns:strataEntity">
	            <xs:sequence>
	                <xs:element name="value" type="xs:string" minOccurs="0" maxOccurs="1" />
	            </xs:sequence>
	            <xs:attribute name="title" type="xs:string" use="optional" />
	        	<xs:attribute name="rel" type="xs:string" use="optional" />
	        	<xs:attribute name="href" type="xs:string" use="optional" />
	        	<xs:attribute name="type" type="xs:string" use="optional" />
	        	<!-- This is not standard and is for backwards compatibility only -->
	        	<xs:attribute name="uri" type="xs:string" use="optional" />
	        </xs:extension>
	    </xs:complexContent>
	</xs:complexType>
	    
    <xs:element name="problems">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of problem reports. This type serves as both a
					top-level construct in the case that a query returns a list of
					problem reports as well as an embedded construct as part of a larger
					item. 
                </xs:documentation>
            </xs:annotation>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="source" type="xs:string" minOccurs="0" maxOccurs="1" />
                <xs:element ref="tns:link" />
                <xs:element ref="tns:problem" />
            </xs:choice>
        </xs:complexType>
    </xs:element>
	
	
    <xs:element name="problem">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A problem report.</xs:documentation>
            </xs:annotation>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="source" type="xs:string" minOccurs="0" maxOccurs="1" />
                <xs:element ref="tns:link" />
            </xs:choice>
        </xs:complexType>
    </xs:element>
	
    <xs:element name="solutions">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of solutions. This type serves as both a
					top-level construct in the case that a query returns a list of
					solutions as well as an embedded construct as part of a larger
					item. 
                </xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:element ref="tns:solution" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="articles">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>A list of articles. This type serves as both a
					top-level construct in the case that a query returns a list of
					articles as well as an embedded construct as part of a larger
					item. 
                </xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:element ref="tns:article" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    	
    <xs:element name="sqiRating">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>An SQI Rating for a KCS Solution</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
                <xs:extension base="tns:trackedEntity">
                    <xs:sequence>

                        <xs:element name="solution" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>A link to the Solution this sqiRating is applicable to.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:simpleContent>
                                    <xs:extension base = "xs:anyURI">
                                        <xs:attribute name="rel" type="xs:string" use="optional" default="solution" />
                                        <xs:attribute name="solutionId" type="xs:string" use="optional" />
                                    </xs:extension>
                                </xs:simpleContent>
                            </xs:complexType>
                        </xs:element>
	
                        <xs:element name="completeTitle" type="xs:integer" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Is the Title Complete?</xs:documentation>
			
                            </xs:annotation>
                        </xs:element>
			            
                        <xs:element name="adheresToContentStandard" type="xs:integer" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Solution Adheres to current template, style, and formatting guidelines in the content standard?</xs:documentation>
                            </xs:annotation>
                        </xs:element>

                        <xs:element name="accuratelyReflectsProblem" type="xs:integer" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Issue accurately reflects customer's problem and context</xs:documentation>
                            </xs:annotation>
                        </xs:element>

                        <xs:element name="isUnique" type="xs:integer" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Is Article Unique?</xs:documentation>
                            </xs:annotation>
                        </xs:element>

                        <xs:element name="clearAndConcise" type="xs:integer" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Is Content Clear and Concise?</xs:documentation>
                            </xs:annotation>
                        </xs:element>

                        <xs:element name="metadataSetCorrectly" type="xs:integer" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Is Article Meta-Data set correctly?</xs:documentation>
                            </xs:annotation>
                        </xs:element>

                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
	
